    "@types/next": "^8.0.3",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/styled-jsx": "^2.2.8",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-mdx": "^1.2.0",
    "@zeit/next-typescript": "^1.1.1",
    "next-compose-plugins": "^2.1.1",
    "next-purgecss": "^3.0.1",
    "typescript": "3.1.1"
  },
